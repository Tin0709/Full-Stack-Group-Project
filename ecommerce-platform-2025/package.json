{
  "name": "ecommerce-platform-2025",
  "version": "1.0.0",
  "private": true,
  "description": "Full-stack ecommerce platform with React frontend and Node.js backend",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start:backend": "cd backend && node index.js",
    "dev:backend": "cd backend && nodemon index.js",
    "start:frontend": "cd frontend && npm run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "build": "cd frontend && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecommerce",
    "react",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "To Fill",
  "license": "MIT",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.7",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.17.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  }
}
